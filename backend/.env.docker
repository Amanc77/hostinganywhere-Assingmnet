# backend/.env.docker (use for docker-compose)
PORT=5000

# when running inside docker-compose, host must be the service name
MYSQL_HOST=db
MYSQL_PORT=3306
MYSQL_USER=appuser
MYSQL_PASSWORD=apppass
MYSQL_DATABASE=hosting_anywhere

# redis service in compose
REDIS_URL=redis://redis:6379

# JWT secret
JWT_SECRET=supersecretkey
